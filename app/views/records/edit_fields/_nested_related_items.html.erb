<% if f.object.class.multiple? key %>
    <%= f.input key, as: :multi_value_ordered, input_html: { class: 'form-control nested-field' }, :wrapper_html => {:class => "nested-field ordered-field-container"} , required: f.object.required?(key) %>
<% else %>
    <%= f.input key, required: f.object.required?(key) %>
<% end %>