<% if f.object.multiple? key %>
    <%= f.input key, as: :multi_value, input_html: { class: 'form-control' }, required: f.object.required?(key) %>
<% else %>
    <%= f.input key, input_html: { class: "form-control degree-grantors-default #{f.object.new_record? ? 'new-record' : ''}" }, required: f.object.required?(key) %>
<% end %>