<% unless presenter.nested_geo.empty? %>
    <tr>
      <th><%= raw t('blacklight.search.form.nested_geo') %></th>
      <td>
        <ul class="tabular">
          <% presenter.nested_geo.each do |coordinates| %>
              <li class="attribute">
        <span itemprop="place" itemscope itemtype="http://schema.org/GeoCoordinates">
         <span itemprop="label"><%= link_to_sa_field('nested_geo_label_ssim', coordinates) %></span>
       </span>
              </li>
          <% end %>
        </ul>
      </td>
    </tr>
<% end %>